	if ($.trim($('#address_mobile').val()) != '' && !Check.isMobile($.trim($('#address_mobile').val()))) {
		alert('请填写正确的手机号码！');
		$('#address_mobile').focus();
		return false;
	}
	
	var province=$.trim($('#province').val());
