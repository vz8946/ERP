		return false;
	}

	var city=$.trim($('#city').val());
	if(city=='' || /\D+/.test(city)){
		alert('请选择城市！');
		$('#city').focus();
		return false;
	}

	var area=$.trim($('#area').val());
	if(area=='' || /\D+/.test(area)){
		alert('请选择地区！');
